     "text": [
      "   company_location         salary salary_currency\n",
      "0                AD   47000.000000             EUR\n",
      "1                AE   86000.000000             USD\n",
      "2                AM   50000.000000             USD\n",
      "3                AR   62444.444444             USD\n",
      "4                AS  470666.666667             USD\n",
      "..              ...            ...             ...\n",
      "72               TR  143666.666667             USD\n",
      "73               UA  105600.000000             USD\n",
      "74               US  158132.304669             USD\n",
      "75               VN   63000.000000             USD\n",
      "76               ZA   76508.157895             GBP\n",
      "\n",
      "[77 rows x 3 columns]\n"
     ]
    }
   ],
   "source": [
    "df_sal = df.groupby('company_location')['salary'].mean().reset_index()\n",
    "currency_info = df.groupby('company_location')['salary_currency'].first().reset_index()\n",
    "\n",
    "result = pd.merge(df_sal, currency_info, on='company_location', how='left')\n",
    "\n",
    "print(result)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "748073c5",
